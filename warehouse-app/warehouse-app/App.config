<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--заголовок программы-->
    <add key="appTitle" value="Склад"/>
    
    <!--путь к директории приложении-->
    <add key="jsonPath" value="C:\Temp\warehouse-app"></add>
    
    <!--интервал времени в минутах, через который должны актуализироваться данные-->
    <add key="cacheExpiration" value="5"></add>
  </appSettings>
</configuration>